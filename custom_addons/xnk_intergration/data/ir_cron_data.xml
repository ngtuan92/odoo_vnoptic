<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="1">
        <record id="ir_cron_sync_products_daily" model="ir.cron">
            <field name="name">Daily Product Sync from Spring Boot</field>
            <field name="model_id" ref="xnk_intergration.model_product_sync"/>
            <field name="state">code</field>
            <field name="code">model.sync_products_from_springboot()</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="active" eval="True"/>
        </record>
    </data>
</odoo>
