<odoo>
    <record id="product_warranty_tree_view" model="ir.ui.view">
        <field name="name">product.warranty.tree</field>
        <field name="model">product.warranty</field>
        <field name="arch" type="xml">
            <tree string="Product Warranty">
                <field name="name"/>
                <field name="cid"/>
                <field name="value"/>
                <field name="warranty_type"/>
                <field name="active"/>
            </tree>
        </field>
    </record>

    <record id="product_warranty_form_view" model="ir.ui.view">
        <field name="name">product.warranty.form</field>
        <field name="model">product.warranty</field>
        <field name="arch" type="xml">
            <form string="Product Warranty">
                <sheet>
                    <group>
                        <group>
                            <field name="name"/>
                            <field name="cid"/>
                            <field name="value"/>
                            <field name="warranty_type"/>
                            <field name="active"/>
                        </group>
                        <group>
                            <field name="description"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="action_product_warranty" model="ir.actions.act_window">
        <field name="name">Product Warranty</field>
        <field name="res_model">product.warranty</field>
        <field name="view_mode">tree,form</field>
    </record>
</odoo>